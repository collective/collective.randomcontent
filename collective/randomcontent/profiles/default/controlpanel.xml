<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.randomcontent">
  <configlet
      title="Random Content Settings"
      action_id="collective.randomcontent.settings"
      appId="collective.randomcontent"
      category="Products"
      condition_expr=""
      url_expr="string:${portal_url}/@@collective-randomcontent-settings"
      icon_expr="string:${portal_url}/product_icon.png"
      visible="True"
      i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
</object>
